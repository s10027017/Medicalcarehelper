<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tw.helper.medicalcare">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <permission android:name="tw.helper.medicalcare.permission.MAPS_RECEIVE"
        android:protectionLevel="signature"></permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.Record_main"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.News_info"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.Fans"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.Carry_info"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.Inquire"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.Schedule"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.Case_info"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.Case_info_basic"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.Case_info_notice"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.Mh02"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.Sign_up_list"></activity>
        <activity
            android:screenOrientation="portrait"
            android:name="tw.helper.medicalcare.News_content"></activity>
    </application>

</manifest>